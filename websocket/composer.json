{
    "name": "easyappointments/websocket-server",
    "description": "WebSocket server for Easy!Appointments real-time notifications",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "cboden/ratchet": "^0.4.4",
        "firebase/php-jwt": "^6.10",
        "react/socket": "^1.15"
    },
    "autoload": {
        "psr-4": {
            "EasyAppointments\\WebSocket\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
